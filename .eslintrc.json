{
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended", "airbnb-base"],
  "plugins": ["react", "import"],
  "rules": {
    "strict": 0,
    "comma-dangle": ["error", "never"]
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "document": true,
    "localStorage": true,
    "window": true,
    "XMLHttpRequest": true
  }
}
